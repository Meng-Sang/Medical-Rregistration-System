    <tag>
        <description>judge this tag to show</description>
        <name>outInvest</name>
        <tag-class>com.sangmeng.tag.OutInvestTag</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>per</name>
            <required>true</required>
        </attribute>
    </tag>
    <tag>
        <description>judge this tag to show</description>
        <name>outNotices</name>
        <tag-class> com.sangmeng.tag.outNoticeTag</tag-class>
        <body-content>scriptless</body-content>
