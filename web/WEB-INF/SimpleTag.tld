        <name>outAdName</name>
        <tag-class>com.sangmeng.tag.OutAdministrativeTag</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>num</name>
            <required>false</required>
        </attribute>
    </tag>
